<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    tools:ignore="MissingPrefix"
    android:clickable="false"
    android:focusable="false"
    android:orientation="vertical">




    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="35dp"
            android:background="#e7e7e7">
            <de.morrox.fontinator.FontTextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:textAllCaps = "true"
                android:text="\@DAKOTA"
                app:font="OpenSans-SemiBold.ttf"
                android:id="@+id/username"
                android:layout_marginLeft="10dp"
                android:textSize="12sp"
                android:textStyle="bold"
                android:gravity="center"
                android:textColor="#666666" />

            <de.morrox.fontinator.FontTextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text="IN"
                app:font="OpenSans-SemiBold.ttf"
                android:id="@+id/textView31"
                android:textSize="12sp"
                android:textStyle="bold"
                android:gravity="center"
                android:textColor="#a9a9a9" />

            <de.morrox.fontinator.FontTextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:textAllCaps = "true"
                app:font="OpenSans-SemiBold.ttf"
                android:text="ENTREPRENEUR"
                android:id="@+id/hubName"
                android:textSize="12sp"
                android:textStyle="bold"
                android:gravity="center"
                android:textColor="#666666" />

            <de.morrox.fontinator.FontTextView
                app:font="OpenSans-SemiBold.ttf"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:textAllCaps="true"
                android:text="posted 2 min ago"
                android:id="@+id/date"
                android:textSize="12sp"
                android:textStyle="bold"
                android:textColor="#666666"
                android:gravity="center|right"
                android:layout_marginRight="10dp" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/relly"
            android:background="#ffffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps = "true"
                android:text="LIVING THE STARTUP LIFE"
                android:id="@+id/title"
                app:font="OpenSans-SemiBold.ttf"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp"
                android:textStyle="bold"
                android:textColor="#5d5d5d" />

            <de.morrox.fontinator.FontTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Medium TextMedium TextMedium TMedium TextMedium TextMedium TextextMedium TextMedium TextMedium TextMedium TextMedium TextMedium TextMedium TextMedium TextMedium TextMedium TextMedium TextMedium Text"
                android:id="@+id/description2"
                app:font="Merriweather-Regular.ttf"

                android:layout_below="@+id/title"
                android:layout_alignLeft="@+id/title"
                android:layout_alignStart="@+id/title"
                android:layout_marginTop="7dp"
                android:layout_marginRight="5dp"
                android:textColor="#5d5d5d"
                android:paddingBottom="10dp"
                android:maxLines="5" />
        </RelativeLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="35dp"
            android:weightSum="6">

            <TextView
                android:layout_width="180dp"
                android:layout_height="fill_parent"
                android:text="12 COMMENTS"
                android:id="@+id/commentNum"
                android:gravity="center"
                android:background="#12a7b3"
                android:textColor="@color/white" />

            <TextView
                android:layout_width="40dp"
                android:layout_height="fill_parent"
                android:text="0"
                android:id="@+id/score"
                android:background="#f5f5f5"
                android:gravity="center" />

            <ImageButton
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:text="V"
                android:id="@+id/hand_up"
                android:background="#e2e2e2"
                android:gravity="center"
                android:src="@drawable/angle_up"
                android:scaleType="center"
                android:clickable="true" />

        </LinearLayout>

    </LinearLayout>
    </LinearLayout>

</LinearLayout>

