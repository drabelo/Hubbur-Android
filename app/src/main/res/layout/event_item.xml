<?xml version="1.0" encoding="utf-8"?>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    tools:ignore="MissingPrefix"
    android:clickable="false"
    android:focusable="false"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#00ffffff">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff">

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imagess">

                <beta.drab.hubber.ect.SquareImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imagee"
                    android:src="@drawable/profile_image"
                    android:scaleType="fitXY"
                    android:focusableInTouchMode="false" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="35dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:textAllCaps = "true"
                        android:text="\@DAKOTA"
                        fontPath="fonts/Open_Sans/OpenSans-SemiBold.ttf"
                        android:id="@+id/username"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:textColor="#666666"
                        android:visibility="invisible" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:text="IN"
                        fontPath="fonts/Open_Sans/OpenSans-SemiBold.ttf"
                        android:id="@+id/textView31"
                        android:layout_marginRight="10dp"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:textColor="#a9a9a9"
                        android:visibility="invisible" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:textAllCaps = "true"
                        fontPath="fonts/Open_Sans/OpenSans-SemiBold.ttf"
                        android:text="ENTREPRENEUR"
                        android:id="@+id/hubName"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:textColor="#666666"
                        android:visibility="invisible" />

                    <TextView
                        fontPath="fonts/Open_Sans/OpenSans-SemiBold.ttf"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAllCaps="true"
                        android:text="posted 2 min ago"
                        android:id="@+id/date"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        android:textColor="#666666"
                        android:gravity="center|right"
                        android:layout_marginRight="10dp"
                        android:visibility="invisible" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="#6b000000">

                    <TextView
                        fontPath="fonts/Merriweather/Merriweather-Regular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAllCaps="true"
                        android:text="LIVING THE STARTUP LIFE"
                        android:id="@+id/titleEventMain"
                        android:layout_alignParentStart="false"
                        android:textColor="#fffffff0"
                        android:layout_alignParentEnd="false"
                        android:layout_gravity="center"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        fontPath="fonts/Merriweather/Merriweather-Regular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAllCaps="true"
                        android:text="UMass Amherst, CS Building"
                        android:id="@+id/locationEventMain"
                        android:layout_alignParentStart="false"
                        android:textColor="#fffffff0"
                        android:layout_alignParentEnd="false"
                        android:layout_gravity="center"
                        android:textSize="16sp" />

                </LinearLayout>

                <TextView
                    fontPath="fonts/Merriweather/Merriweather-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAllCaps="true"
                    android:text="APRIL 17TH 2015 1:00PM"
                    android:id="@+id/dateEventMain"
                    android:layout_alignParentStart="false"
                    android:textColor="#fffffff0"
                    android:layout_alignParentEnd="false"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="13sp"
                    android:layout_marginBottom="10dp" />

            </FrameLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:background="#ff4db6ac"
                android:id="@+id/relativeLayout"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:alpha=".8"
                android:layout_gravity="bottom">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" >

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Comments"
                        android:id="@+id/textView40"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="16sp"
                        android:textColor="#ffffff"
                        android:background="@android:color/holo_blue_dark" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="GO"
                        android:id="@+id/going"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="16sp"
                        android:textColor="#ffffff"
                        android:background="#ff0eba00" />
                </LinearLayout>
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

</LinearLayout>


